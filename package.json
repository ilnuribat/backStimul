{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node src/main/index.js",
    "dev": "nodemon src",
    "databases:up": "docker-compose -f dev/docker-compose.yml up --build -d",
    "databases:down": "docker-compose -f dev/docker-compose.yml down --volumes",
    "migrate:add": "node src/utils/createMigration",
    "migrate:up": "npx sequelize db:migrate",
    "migrate:down": "npx sequelize db:migrate:undo",
    "services:up": "docker-compose -f dev/services.yml up -d",
    "services:down": "docker-compose -f dev/services.yml down"
  },
  "author": "",
  "license": "ISC",
  "nodemonConfig": {
    "ignore": [
      "dev/*",
      "node_modules/*"
    ]
  },
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1"
  },
  "dependencies": {
    "@koa/cors": "^2.2.2",
    "bcrypt": "^2.0.1",
    "boom": "^7.2.0",
    "cookie": "^0.3.1",
    "dotenv": "^6.0.0",
    "graphql": "^14.0.2",
    "graphql-tools": "^3.1.1",
    "ioredis": "^4.0.0",
    "jsonwebtoken": "^8.3.0",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-graphql": "^0.7.5",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.10",
    "merge-graphql-schemas": "^1.5.1",
    "moleculer": "^0.13.2",
    "moleculer-repl": "^0.5.2",
    "moment": "^2.22.2",
    "mongoose": "^5.2.9",
    "nodemon": "^1.18.4",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.10",
    "sequelize-cli": "^4.1.1",
    "socket.io": "^2.1.1",
    "uuid": "^3.3.2",
    "winston": "^3.1.0"
  }
}
