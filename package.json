{
  "name": "stimul",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "databases:up": "docker-compose -f dev/docker-compose.yml up --build -d",
    "databases:down": "docker-compose -f dev/docker-compose.yml down --volumes",
    "migrate:add": "node migrations/createMigration",
    "migrate:up": "node migrations/index.js",
    "test": "NODE_ENV=test LOG_LEVEL=error nyc --reporter=text-summary --reporter=html mocha test/init.js --exit test/*",
    "test:watch": "cd test && NODE_ENV=test MONGO_HOST=localhost mocha init.js -w * || cd ../",
    "lint": "npx eslint ."
  },
  "author": "StimulTeam",
  "license": "ISC",
  "nodemonConfig": {
    "ignore": [
      "dev/*",
      "node_modules/*",
      "client",
      "test"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "eslint": "5.9.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-mocha": "5.2.1"
  },
  "dependencies": {
    "activedirectory": "^0.7.2",
    "apollo-server": "2.2.5",
    "apollo-server-express": "2.2.5",
    "apollo-upload-server": "7.1.0",
    "axios": "0.18.0",
    "bcrypt": "3.0.2",
    "bluebird": "3.5.3",
    "body-parser": "1.18.3",
    "chai": "4.2.0",
    "chai-http": "4.2.0",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "filesize": "3.6.1",
    "graphql": "14.0.2",
    "graphql-subscriptions": "1.0.0",
    "graphql-tools": "4.0.3",
    "husky": "^1.3.0",
    "ioredis": "4.3.0",
    "istanbul": "0.4.5",
    "jsonwebtoken": "8.4.0",
    "knex": "0.14",
    "lodash": "4.17.11",
    "merge-graphql-schemas": "1.5.8",
    "mocha": "5.2.0",
    "moment": "2.22.2",
    "mongodb": "3.1.10",
    "mongoose": "5.3.16",
    "nodemon": "1.18.8",
    "nyc": "13.1.0",
    "pg": "7.7.1",
    "subscriptions-transport-ws": "0.9.15",
    "winston": "3.1.0",
    "ws": "6.1.2"
  }
}
