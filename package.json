{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node src/main/index.js",
    "dev": "nodemon server",
    "databases:up": "docker-compose -f server/dev/docker-compose.yml up --build -d",
    "databases:down": "docker-compose -f server/dev/docker-compose.yml down --volumes",
    "migrate:add": "node src/utils/createMigration",
    "migrate:up": "npx sequelize db:migrate",
    "migrate:down": "npx sequelize db:migrate:undo",
    "services:up": "docker-compose -f dev/services.yml up -d",
    "services:down": "docker-compose -f dev/services.yml down",
    "test": "NODE_ENV=test nyc --reporter=html mocha test/init.js --exit test/*"
  },
  "author": "",
  "license": "ISC",
  "nodemonConfig": {
    "ignore": [
      "dev/*",
      "node_modules/*",
      "client",
      "test"
    ]
  },
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0"
  },
  "dependencies": {
    "apollo-client": "^2.4.6",
    "apollo-link": "^1.2.3",
    "apollo-link-ws": "^1.0.9",
    "apollo-server": "^2.1.0",
    "apollo-server-express": "^2.2.2",
    "axios": "^0.18.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.4",
    "filesize": "^3.6.1",
    "graphql": "^14.0.2",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tools": "^3.1.1",
    "ioredis": "^4.0.0",
    "iterall": "^1.2.2",
    "jsonwebtoken": "^8.3.0",
    "knex": "^0.15.2",
    "lodash": "^4.17.10",
    "merge-graphql-schemas": "^1.5.1",
    "mocha": "^5.2.0",
    "moleculer": "^0.13.2",
    "moleculer-repl": "^0.5.2",
    "moment": "^2.22.2",
    "mongoose": "^5.2.9",
    "nodemon": "^1.18.4",
    "nyc": "^13.1.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "subscriptions-transport-ws": "^0.9.15",
    "uuid": "^3.3.2",
    "winston": "^3.1.0",
    "ws": "^6.1.2"
  }
}
