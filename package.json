{
  "scripts": {
    "start": "node src/main/index.js",
    "dev": "nodemon server",
    "databases:up": "docker-compose -f dev/docker-compose.yml up --build -d",
    "databases:down": "docker-compose -f dev/docker-compose.yml down --volumes",
    "migrate:add": "node src/utils/createMigration",
    "migrate:up": "npx sequelize db:migrate",
    "migrate:down": "npx sequelize db:migrate:undo",
    "services:up": "docker-compose -f dev/services.yml up -d",
    "services:down": "docker-compose -f dev/services.yml down"
  },
  "nodemonConfig": {
    "ignore": [
      "dev/*",
      "node_modules/*"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.2.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "apollo-server": "^2.0.0",
    "bcrypt": "^3.0.0",
    "dotenv": "^4.0.0",
    "express-jwt": "^5.3.0",
    "faker": "^4.1.0",
    "graphql": "^0.13.2",
    "graphql-date": "^1.0.3",
    "iterall": "^1.2.2",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.4",
    "mongoose": "^5.2.17",
    "pg": "^7.4.3",
    "sequelize": "^4.4.2",
    "sqlite3": "^4.0.1",
    "start": "^5.1.0"
  }
}
